@{
    Layout = null; // do not use master page
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="~/Content/styles.css">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,700,800&display=swap" rel="stylesheet">     
    <title>Table Tennis World</title>
</head>
<body>
    <header class="header">
        
        <h1>Table tennis world</h1>
        @Html.ActionLink("News", "GetAllNews", "News")
        @Html.ActionLink("Events", "GetAllEvents", "Events")
        @Html.ActionLink("Ranking", "GetRanking", "Ranking")

        @Html.Partial("_LoginPartial")
        <div class="header_icon"></div>
        <div class="header_icon-stiga"></div>
    </header>
    <main class="main">
        <div id="loading" style="display:none">loading...</div>
        
        <div class="main-content_wrapper">
            
            <section class="main-content_section">
                <div class="title-container">
                    <h3 class="main-content_section_title" data-component="news-title">News</h3>
                </div>
                <div class="news-section" data-component="news-section-container"> 
                </div> 
                
            </section>
                <section class="main-content_section">
                    <div class="title-container">
                        <a href="#events">
                            <h3 class="main-content_section_title" data-component="events-title">Events</h3>
                        </a>
                        <a name="events"></a>
                    </div>
                        <div class="events-section" data-component="events-section-container">

                        </div>
                </section>
                
                <section class="main-content_section">
                    <div class="title-container">
                        <h3 class="main-content_section_title" data-component="players-title">Players</h3>
                    </div>
                    <div class="players-section" data-component="player-section-container">
                        
                    </div>
                </section>
            
               </div>
            </main>
    <script src="~/Scripts/jquery-1.10.2.min.js")"></script>
   

    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>
    <script src="~/Scripts/AppScripts/getNewsClickHandler.js"></script>
    <script src="~/Scripts/AppScripts/getEventsClictHandler.js"></script>
    <script src="~/Scripts/AppScripts/getRankingClickHandler.js"></script>

    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>